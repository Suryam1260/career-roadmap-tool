  "matching_algorithm": {
    "description": "Algorithm to select best matching persona from quiz responses",
    "process": [
      "1. Calculate match score for each persona based on matching_criteria",
      "2. Weight criteria: userType (30%), yearsOfExperience (25%), targetRole (35%), requirementType (10%)",
      "3. Exact matches = 1.0, partial matches = 0.5, no match = 0",
      "4. Select persona with highest score",
      "5. If tie: prefer more specific match (more matching criteria met)",
      "6. Fallback: match on targetRole alone if needed"
    ]
  },

  "company_type_modifiers": {
    "description": "How company type tweaks persona content (not separate config)",
    "usage": "Applied WITHIN persona config via if_companyType conditional fields",
    "types": {
      "startup": {
        "emphasis": ["speed", "learning", "impact"],
        "skip_topics": ["legacy_systems", "enterprise_processes"],
        "accelerate_topics": ["mvp_thinking", "rapid_iteration"]
      },
      "scaleup": {
        "emphasis": ["scaling", "system_design", "reliability"],
        "skip_topics": ["basic_crud"],
        "accelerate_topics": ["microservices", "distributed_systems"]
      },
      "bigtech": {
        "emphasis": ["system_design", "scale", "interviews"],
        "skip_topics": ["basic_web_dev"],
        "accelerate_topics": ["advanced_algorithms", "architecture"]
      }
    }
